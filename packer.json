{
  "variables": {
    "aws_access_key": "AWS_ACCESS_KEY_ID",
    "aws_secret_key": "AWS_SECRET_ACCESS_KEY",
    "packer_build_script":  "build_script"
  },
  "builders": [
    {
      "type": "amazon-ebs",
      "access_key": "{{user `aws_access_key`}}",
      "secret_key": "{{user `aws_secret_key`}}",
      "region": "eu-west-2",
      "source_ami": "ami-03603cffd09f888ee",
      "instance_type": "t3.micro",
      "ssh_username": "ec2-user",
      "ami_name": "packer-az2 {{timestamp}}",
      "security_group_id": "sg-0a533f90b1ab67030",
      "ssh_timeout": "10m",
      "subnet_id": "subnet-f12cb98b"
    }
  ],
  "provisioners": [
      {
        "type": "shell",
        "script": "{{user `packer_build_script`}}"
      }
    ]
  
}

